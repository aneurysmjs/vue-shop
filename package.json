{
  "name": "vue-shop",
  "type": "module",
  "version": "1.0.0",
  "author": "Аневризма <aneurysmjs@outlook.com>",
  "license": "MIT",
  "repository": "git@github.com:aneurysmjs/vue-shop.git",
  "main": "src/main.ts",
  "scripts": {
    "analize": "npx vite-bundle-visualizer",
    "commit": "git-cz",
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@heroicons/vue": "^2.1.3",
    "@tanstack/vue-query": "^5.45.0",
    "@tanstack/vue-table": "^8.17.0",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "10.9.0",
    "axios": "^1.6.8",
    "dayjs": "^1.11.11",
    "lodash.debounce": "^4.0.8",
    "vee-validate": "^4.12.8",
    "vue": "3.4.27",
    "vue-router": "4.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.20.0",
    "@iconify-json/carbon": "1.1.33",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/vue": "^8.0.3",
    "@tsconfig/node18": "^18.2.4",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "20.12.11",
    "@unocss/eslint-config": "0.61.0",
    "@unocss/preset-wind": "^0.61.0",
    "@unocss/reset": "0.61.0",
    "@unocss/transformer-directives": "^0.61.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "9.2.0",
    "eslint-plugin-perfectionist": "^2.10.0",
    "eslint-plugin-readable-tailwind": "^1.3.2",
    "jsdom": "24.0.0",
    "json-server": "1.0.0-beta.0",
    "lint-staged": "15.2.2",
    "pnpm": "9.1.1",
    "simple-git-hooks": "2.11.1",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "taze": "0.13.8",
    "typescript": "5.4.5",
    "unocss": "0.61.0",
    "unplugin-auto-import": "0.17.6",
    "unplugin-vue-components": "0.27.0",
    "unplugin-vue-router": "^0.8.6",
    "vite": "5.3.1",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "1.6.0",
    "vue-tsc": "2.0.17"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
